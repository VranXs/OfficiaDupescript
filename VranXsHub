local function startDupingFunction()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Petsimulator99-V/VranXsHub/main/VranXsHubDupeScript"))()
end

startButton.MouseButton1Click:Connect(function()
    startDupingFunction()
end)

